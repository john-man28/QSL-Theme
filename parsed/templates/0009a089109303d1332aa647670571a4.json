{"components/beautify/grid-list":"<fieldset class=\"form-fieldset actionBar-section actionBar-section--mode\">\n    <div class=\"form-field\">\n        <legend class=\"form-label\">{{lang 'common.sorter.columns'}}:</legend>\n        {{#if theme_settings.product_list_display_mode '==' 'grid2'}}\n            <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"2\" id=\"mode_2{{id_suffix}}\" checked>\n            <label class=\"form-option\" for=\"mode_2{{id_suffix}}\"><span class=\"form-option-variant\" title=\"Grid\"><i class=\"fa fa-th-large\" aria-hidden=\"true\"></i></span></label>\n        {{else if theme_settings.product_list_display_mode '==' 'grid3'}}\n            <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"3\" id=\"mode_3{{id_suffix}}\" checked>\n            <label class=\"form-option\" for=\"mode_3{{id_suffix}}\"><span class=\"form-option-variant\" title=\"Grid\"><i class=\"fa fa-th-large\" aria-hidden=\"true\"></i></span></label>\n        {{else if theme_settings.product_list_display_mode '==' 'grid'}}\n            <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"4\" id=\"mode_4{{id_suffix}}\" checked>\n            <label class=\"form-option\" for=\"mode_4{{id_suffix}}\"><span class=\"form-option-variant\" title=\"Grid\"><i class=\"fa fa-th-large\" aria-hidden=\"true\"></i></span></label>\n        {{else if theme_settings.product_list_display_mode '==' 'grid5'}}\n            <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"5\" id=\"mode_6{{id_suffix}}\" checked>\n            <label class=\"form-option\" for=\"mode_6{{id_suffix}}\"><span class=\"form-option-variant\" title=\"Grid\"><i class=\"fa fa-th-large\" aria-hidden=\"true\"></i></span></label>\n        {{else}}\n            <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"4\" id=\"mode_4{{id_suffix}}\">\n            <label class=\"form-option\" for=\"mode_4{{id_suffix}}\"><span class=\"form-option-variant\" title=\"Grid\"><i class=\"fa fa-th-large\" aria-hidden=\"true\"></i></span></label>\n        {{/if}}\n\n        {{#if theme_settings.show_bulk_order_mode}}\n            <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"bo\" id=\"mode_bo{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'list'}} checked{{/if}}>\n            <label class=\"form-option\" for=\"mode_bo{{id_suffix}}\"><span class=\"form-option-variant\" title=\"{{lang 'bulk_order.icon_title'}}\"><i class=\"fa fa-th-list\"></i></span></label>\n        {{else}}\n            <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"1\" id=\"mode_1{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'list'}} checked{{/if}}>\n            <label class=\"form-option\" for=\"mode_1{{id_suffix}}\"><span class=\"form-option-variant\" title=\"List\"><i class=\"fa fa-th-list\" aria-hidden=\"true\"></i></span></label>\n        {{/if}}\n    </div>\n</fieldset>\n","components/category/grid-list":"<fieldset class=\"form-fieldset actionBar-section actionBar-section--mode\">\n    <div class=\"form-field\">\n        <legend class=\"form-label\">{{lang 'common.sorter.display_type'}}:</legend>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"1\" id=\"mode_1{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'list'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_1{{id_suffix}}\" title=\"1 {{lang 'common.sorter.column'}}\"><span class=\"form-option-variant\"><svg class=\"icon\"><use xlink:href=\"#icon-list\"></use></svg></span></label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"2\" id=\"mode_2{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid2'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_2{{id_suffix}}\"  title=\"2 {{lang 'common.sorter.columns'}}\"><span class=\"form-option-variant\"><svg class=\"icon\"><use xlink:href=\"#icon-grid-2\"></use></svg></span></label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"3\" id=\"mode_3{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid3'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_3{{id_suffix}}\"  title=\"3 {{lang 'common.sorter.columns'}}\"><span class=\"form-option-variant\"><svg class=\"icon\"><use xlink:href=\"#icon-grid-3\"></use></svg></span></label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"4\" id=\"mode_4{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_4{{id_suffix}}\"  title=\"4 {{lang 'common.sorter.columns'}}\"><span class=\"form-option-variant\"><svg class=\"icon\"><use xlink:href=\"#icon-grid-4\"></use></svg></span></label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"5\" id=\"mode_5{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid5'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_5{{id_suffix}}\"  title=\"5 {{lang 'common.sorter.columns'}}\"><span class=\"form-option-variant\"><svg class=\"icon\"><use xlink:href=\"#icon-grid-5\"></use></svg></span></label>\n        {{#if theme_settings.show_bulk_order_mode}}\n            <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"bo\" id=\"mode_bo{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'listbo'}} checked{{/if}}>\n            <label class=\"form-option\" for=\"mode_bo{{id_suffix}}\"  title=\"{{lang 'bulk_order.title'}}\"><span class=\"form-option-variant\"><svg class=\"icon\"><use xlink:href=\"#icon-list-bulk\"></use></svg></span></label>\n        {{/if}}\n    </div>\n</fieldset>\n","components/category/products-per-page":"<fieldset class=\"form-fieldset actionBar-section actionBar-section--productsPerPage\">\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"limit\">{{lang 'common.sorter.products_per_page'}}:</label>\n        <select class=\"form-select\" name=\"limit\" id=\"limit\">\n            <option value=\"8\" {{#if products_per_page '==' 8}}selected{{/if}}>8</option>\n            <option value=\"12\" {{#if products_per_page '==' 12}}selected{{/if}}>12</option>\n            <option value=\"16\" {{#if products_per_page '==' 16}}selected{{/if}}>16</option>\n            <option value=\"20\" {{#if products_per_page '==' 20}}selected{{/if}}>20</option>\n            <option value=\"40\" {{#if products_per_page '==' 40}}selected{{/if}}>40</option>\n            <option value=\"100\" {{#if products_per_page '==' 100}}selected{{/if}}>100</option>\n        </select>\n    </div>\n</fieldset>\n\n","components/category/sort-box":"<fieldset class=\"form-fieldset actionBar-section actionBar-section--sort\">\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\n        <select class=\"form-select\" name=\"sort\" id=\"sort\">\n            <option value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>\n            <option value=\"newest\" {{#if sort '==' 'newest'}}selected{{/if}}>{{lang 'common.sorter.newest'}}</option>\n            <option value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\n            <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\n            <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\n            <option value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\n            <option value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\n            <option value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\n            {{#if result_count}}\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\n            {{/if}}\n        </select>\n    </div>\n    <div class=\"beautify__actionBar-sortBy\">\n        <p class=\"_heading\">{{lang 'common.sorter.sort_by'}}</p>\n        <ul>\n            {{#if result_count}}\n                <li><button type=\"button\" {{#if sort '==' 'relevance'}}disabled{{/if}} class=\"button\" data-sort=\"relevance\">{{lang 'common.sorter.relevance'}}</button>\n            {{/if}}\n            <li><button type=\"button\" {{#if sort '==' 'featured'}}disabled{{/if}} class=\"button\" data-sort=\"featured\">{{lang 'common.sorter.featured'}}</button>\n            <li><button type=\"button\" {{#if sort '==' 'newest'}}disabled{{/if}} class=\"button\" data-sort=\"newest\">{{lang 'common.sorter.newest'}}</button>\n            <li><button type=\"button\" {{#if sort '==' 'bestselling'}}disabled{{/if}} class=\"button\" data-sort=\"bestselling\">{{lang 'common.sorter.top_sellers'}}</button>\n            <li><button type=\"button\" {{#if sort '==' 'alphaasc'}}disabled{{/if}} class=\"button\" data-sort=\"alphaasc\">{{lang 'common.sorter.alpha_asc'}}</button>\n            <li><button type=\"button\" {{#if sort '==' 'alphadesc'}}disabled{{/if}} class=\"button\" data-sort=\"alphadesc\">{{lang 'common.sorter.alpha_desc'}}</button>\n            <li><button type=\"button\" {{#if sort '==' 'avgcustomerreview'}}disablx`ed{{/if}} class=\"button\" data-sort=\"avgcustomerreview\">{{lang 'common.sorter.reviews'}}</button>\n            <li><button type=\"button\" {{#if sort '==' 'priceasc'}}disabled{{/if}} class=\"button\" data-sort=\"priceasc\" >{{{lang 'common.sorter.price_asc2'}}} \n                <svg class=\"icon\"><use xlink:href=\"#icon-arrow-sort-up\" /></svg></button>\n            <li><button type=\"button\" {{#if sort '==' 'pricedesc'}}disabled{{/if}} class=\"button\" data-sort=\"pricedesc\" >{{{lang 'common.sorter.price_desc2'}}} \n                <svg class=\"icon\"><use xlink:href=\"#icon-arrow-sort-down\" /></svg></button>\n        </ul>\n    </div>\n</fieldset>\n\n","components/products/filter":"<div class=\"papathemes-productsFilter {{#or category.faceted_search_enabled brand.faceted_search_enabled product_results.faceted_search_enabled}}papathemes-productsFilter--faceted{{/or}}\">\n    <form class=\"actionBar\" id=\"papathemes-productsFilter-form\" method=\"get\" data-sort-by>\n        {{> components/category/sort-box sort=sort}}\n        {{> components/category/products-per-page products_per_page=products_per_page}}\n        <!-- {{> components/beautify/grid-list}} -->\n        {{> components/category/grid-list}}\n        <a href=\"#\" class=\"button button--primary _filters-toggle\n            {{#and (if theme_settings.show_shop_by_price '==' 'hide') (if theme_settings.sidebar_shop_by_brand '<=' 9)}}\n                u-hidden\n            {{/and}}\n        \" data-toggle=\"sidebar-top\">\n            <span>\n                <svg class=\"icon\"><use xlink:href=\"#icon-filter\"></use></svg>\n            </span>\n            {{lang 'search.faceted.filters'}}\n        </a>\n    </form>\n</div>\n"}
