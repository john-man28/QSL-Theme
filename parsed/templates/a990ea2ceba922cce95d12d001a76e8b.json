{
  "components/common/product-share": "{{#or settings.add_this.buttons settings.facebook_like_button.enabled}}\n    {{assignVar 'encodedUrl' (encodeURI product.url)}}\n    {{assignVar 'encodedTitle' (encodeURI head.title)}}\n    <div class=\"form-share\">\n        <a href=\"#\" class=\"button dropdown-menu-button\" data-dropdown=\"share-dropdown--{{product.id}}\" aria-controls=\"share-dropdown--{{product.id}}\" aria-expanded=\"false\">\n            <svg class=\"icon\"><use href=\"#icon-share\" /></svg>\n            <span class=\"is-srOnly\">{{lang 'common.share'}}</span>\n        </a>\n        <div class=\"socialLinks socialLinks--alt\" id=\"share-dropdown--{{product.id}}\" tabindex=\"-1\">\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"share_link--{{product.id}}\">{{lang 'common.share'}}</label>\n                <input class=\"form-input\" type=\"text\" id=\"share_link--{{product.id}}\" name=\"share_link--{{product.id}}\" value=\"{{product.url}}\">\n            </div>\n        </div>\n    </div>\n{{/or}}"
}