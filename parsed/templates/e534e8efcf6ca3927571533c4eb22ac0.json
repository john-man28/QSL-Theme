{
  "components/account/edit-account": "<form action=\"{{urls.account.update_action}}\" data-edit-account-form class=\"form\" method=\"post\">\n    {{inject 'firstName' (lang 'forms.validate.account.edit.first_name')}}\n    {{inject 'lastName' (lang 'forms.validate.account.edit.last_name')}}\n    {{inject 'phoneNumber' (lang 'forms.validate.account.edit.phone')}}\n    {{inject 'currentPassword' (lang 'forms.validate.account.edit.password')}}\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-row form-row--half\">\n            <div class=\"form-field\" id=\"account_firstname_id\">\n                <label class=\"form-label\" for=\"account_firstname\">\n                    {{lang 'account.settings.first_name' }}\n                    <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n                </label>\n                <input aria-labelledby=\"account_firstname_id\" aria-live=\"polite\" type=\"text\" class=\"form-input\" name=\"account_firstname\" id=\"account_firstname\" value=\"{{{sanitize forms.edit_account.first_name}}}\">\n            </div>\n            <div class=\"form-field\" id=\"account_lastname_id\">\n                <label class=\"form-label\" for=\"account_lastname\">\n                    {{lang 'account.settings.last_name' }}\n                    <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n                </label>\n                <input aria-labelledby=\"account_lastname_id\" aria-live=\"polite\" type=\"text\" class=\"form-input\" name=\"account_lastname\" id=\"account_lastname\" value=\"{{{sanitize forms.edit_account.last_name}}}\">\n            </div>\n            <div class=\"form-field\" id=\"account_companyname_id\">\n                <label class=\"form-label\" for=\"account_companyname\">\n                    {{lang 'account.settings.company' }}\n                </label>\n                <input aria-labelledby=\"account_companyname_id\" aria-live=\"polite\" type=\"text\" class=\"form-input\" name=\"account_companyname\" id=\"account_companyname\" value=\"{{{sanitize forms.edit_account.company_name}}}\">\n            </div>\n            <div class=\"form-field\" id=\"account_phone_id\">\n                <label class=\"form-label\" for=\"account_phone\">\n                    {{lang 'account.settings.phone' }}\n                </label>\n                <input aria-labelledby=\"account_phone_id\" aria-live=\"polite\" type=\"text\" class=\"form-input\" name=\"account_phone\" id=\"account_phone\" value=\"{{{sanitize forms.edit_account.phone}}}\">\n            </div>\n            {{#each forms.edit_account.fields}}\n                {{{dynamicComponent 'components/common/forms'}}}\n            {{/each}}\n        </div>\n    </fieldset>\n    <div class=\"form-actions\">\n        <button class=\"button button--primary\">{{lang 'account.settings.update_details'}}</button>\n    </div>\n</form>\n",
  "components/common/forms/checkbox": "<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}\">{{label}}\n        {{#if required}}\n            <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n        {{/if}}\n    </label>\n    {{#if checkboxes}}\n        {{#each checkboxes}}\n            <input type=\"checkbox\" value=\"{{value}}\" name=\"{{name}}\" id=\"{{name}}\" data-label=\"{{label}}\" class=\"form-checkbox\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" {{#if checked}}checked{{/if}} {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\n            <label class=\"form-label {{class}}\" for=\"{{name}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n",
  "components/common/forms/date-options": "{{#unless this.value}}\n    <option value=\"\">---</option>\n    {{#each this.items}}\n        <option value=\"{{value}}\">{{label}}</option>\n    {{/each}}\n{{else}}\n    {{#unless this.isRequired}}\n        <option value=\"\">---</option>\n    {{/unless}}\n    {{#each this.items}}\n        <option {{#if ../this.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\n    {{/each}}\n{{/unless}}",
  "components/common/forms/date": "<div class=\"form-field form-field--date\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}\" >{{label}}\n    {{#if required}}<small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>{{/if}}</label>\n    <div class=\"form-row form-row--third\">\n        <div class=\"form-field\">\n            <select class=\"form-select\" name=\"{{month.name}}\" id=\"{{id}}_month\" data-label=\"month\"  aria-required=\"{{required}}\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\n                {{> components/common/forms/date-options month isRequired=required}}\n            </select>\n        </div>\n        <div class=\"form-field\">\n            <select class=\"form-select\" name=\"{{day.name}}\" id=\"{{id}}_day\" data-label=\"day\" aria-required=\"{{required}}\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\n                {{> components/common/forms/date-options day isRequired=required}}\n            </select>\n        </div>\n        <div class=\"form-field\">\n            <select class=\"form-select\" name=\"{{year.name}}\" id=\"{{id}}_year\" data-label=\"year\" aria-required=\"{{required}}\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\n                {{> components/common/forms/date-options year isRequired=required}}\n            </select>\n        </div>\n    </div>\n</div>\n",
  "components/common/forms/number": "<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n        {{#if required}}<small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>{{/if}}\n    </label>\n    <input type=\"number\" class=\"form-input\" data-label=\"{{label}}\" data-rule=\"lowerThan\" min=\"{{limitfrom}}\" max=\"{{limitto}}\" title=\"{{lang 'forms.range' limitFrom=limitfrom limitTo=limitto}}\" value=\"{{value}}\" id=\"{{id}}_input\" name=\"{{name}}\" aria-required=\"{{required}}\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\n</div>\n",
  "components/common/forms/radio": "<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\n    <label class=\"form-label\">{{label}}\n        {{#if required}}\n            <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n        {{/if}}\n    </label>\n    {{#if radios}}\n        {{#each radios}}\n            <input \n                type=\"radio\"\n                class=\"form-radio\"\n                name=\"{{../name}}\"\n                id=\"{{../name}}_{{@index}}\"\n                value=\"{{value}}\"\n                data-label=\"{{label}}\"\n                aria-required=\"{{../required}}\"\n                aria-labelledby=\"{{id}}\"\n                aria-live=\"polite\"\n                {{#if checked}}\n                    checked\n                {{/if}}\n                {{#if private_id}}\n                    data-field-type=\"{{private_id}}\"\n                {{/if}}>\n            <label class=\"form-label\" for=\"{{../name}}_{{@index}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n",
  "components/common/forms/multiline": "<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n        {{#if required}}\n            <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n        {{/if}}\n    </label>\n    <textarea name=\"{{name}}\" id=\"{{id}}_input\" data-label=\"{{label}}\" rows=\"{{rows}}\" aria-required=\"{{required}}\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" class=\"form-input\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>{{{sanitize value}}}</textarea>\n</div>\n",
  "components/common/forms/select": "<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}} data-type=\"{{private_id}}\"\n    {{/if}}>\n    <label class=\"form-label\" for=\"{{id}}_select\">{{label}}\n        {{#if required}}<small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>{{/if}}\n    </label>\n    <select class=\"{{class}} form-select\" id=\"{{id}}_select\" data-label=\"{{label}}\" aria-required=\"{{required}}\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" name=\"{{name}}\"\n        {{#if private_id}} data-field-type=\"{{private_id}}\" {{/if}}>\n        {{#if options}}\n        <option value=\"\">{{chooseprefix}}</option>\n        {{#each options}}\n        <option {{#if selected}} selected {{/if}} value=\"{{value}}\">{{{label}}}</option>\n        {{/each}}\n        {{/if}}\n    </select>\n</div>\n",
  "components/common/forms/selectortext": "<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <noscript>\n        <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n            {{#if required}}<small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>{{/if}}\n        </label>\n        <input type=\"text\" id=\"{{id}}_input\" name=\"{{name}}\" value=\"{{value}}\" class=\"form-input\" aria-required=\"{{required}}\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" data-label=\"{{label}}\" />\n    </noscript>\n\n    <label class=\"form-label\" for=\"{{id}}_select\">{{label}}\n        {{#if required}}\n            <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n        {{/if}}\n    </label>\n    <select name=\"{{name}}\" class=\"form-select\" aria-required=\"{{required}}\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" id=\"{{id}}_select\" data-label=\"{{label}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\n        {{#if options}}\n            <option value=\"\">{{chooseprefix}}</option>\n            {{#each options}}\n                <option {{#if selected}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\n            {{/each}}\n        {{/if}}\n    </select>\n</div>\n",
  "components/common/forms/text": "<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n        {{#if required}}<small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>{{/if}}\n    </label>\n    <input type=\"text\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" {{#if value}} value=\"{{{sanitize value}}}\"{{/if}} {{#if placeholder}} placeholder=\"{{placeholder}}\"{{/if}} class=\"form-input\" aria-required=\"{{required}}\" aria-labelledby=\"{{id}}\" aria-live=\"polite\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}} {{#if maxlength}}maxlength=\"{{maxlength}}\"{{/if}}>\n</div>\n",
  "components/common/forms/password": "<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n        {{#if required}}\n            <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n        {{/if}}\n    </label>\n    <input type=\"password\"\n        class=\"form-input\"\n        id=\"{{id}}_input\"\n        data-label=\"{{label}}\"\n        name=\"{{name}}\"\n        value=\"{{value}}\"\n        autocomplete=\"off\"\n        aria-required=\"{{required}}\"\n        aria-labelledby=\"{{id}}\"\n        aria-live=\"polite\"\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n    >\n</div>\n"
}