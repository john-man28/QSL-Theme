{"components/products/card-placeholder":"<article class=\"card card--placeholder {{#if alternate}}card--alternate{{/if}} {{#if theme_settings.card_show_border}}_border{{/if}} {{#unless theme_settings.card_show_button}}_hideBtn{{/unless}}\">\n    <figure class=\"card-figure\">\n        <div class=\"card-img-container\">\n            <img class=\"card-image\" src=\"data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==\" alt=\"loading\" {{getVar 'productCardImageSizeAttrs'}} />\n        </div>\n        <figcaption class=\"card-figcaption\">\n            <div class=\"card-figcaption-body\">\n                <div class=\"card-buttons card-buttons--alt\">\n                    {{#unless hide_product_quick_view}}\n                        {{#if theme_settings.show_product_quick_view}}\n                            <a class=\"button card-figcaption-button quickview\" tabindex=\"-1\"><svg class=\"icon\"><use xlink:href=\"#icon-bs-search\"></use></svg>{{lang 'products.quick_view'}}</a>\n                        {{/if}}\n                    {{/unless}}\n                    {{#if show_compare}}\n                        <button type=\"button\" class=\"button card-figcaption-button _compare2\" disabled><svg class=\"icon\"><use xlink:href=\"#icon-compare\"></use></svg>{{lang 'products.compare'}}</button>\n                    {{/if}}\n                </div>\n            </div>\n        </figcaption>\n    </figure>\n    <div class=\"card-body\">\n        <h3 class=\"card-title\"></h3>\n\n        {{#if show_rating}}\n            <p class=\"card-text card-text--rating\" data-test-info-type=\"productRating\">\n                <span class=\"rating--small\"></span>\n            </p>\n        {{/if}}\n\n        {{#if theme_settings.card_show_sku}}\n            <div class=\"card-text card-text--sku\" data-test-info-type=\"sku\"></div>\n        {{/if}}\n\n        {{#if theme_settings.card_show_brand}}\n            <div class=\"card-text card-text--brand\" data-test-info-type=\"brandName\"></div>\n        {{/if}}\n\n        {{#if theme_settings.card_custom_fields '!==' ''}}\n            <div class=\"card-text card-text--info\">\n                <div>\n                    <span class=\"card-info-name card-info-name\"></span>\n                    <span class=\"card-info-value card-info-value\"></span>\n                </div>\n            </div>\n        {{/if}}\n\n        {{#if theme_settings.card_show_swatches}}\n            <div class=\"card-text card-text--colorswatches\"></div>\n        {{/if}}\n\n        <div class=\"card-text card-text--price\" data-test-info-type=\"price\"></div>\n\n        <div class=\"card-text card-text--summary\" data-test-info-type=\"summary\"></div>\n    </div>\n    <div class=\"card-footer{{#if theme_settings.card_show_button}} _show{{/if}}\">\n        <div class=\"card-buttons\">\n                {{#or customer (unless settings.hide_price_from_guests)}}\n                    {{#if show_cart_action}}\n                        <div class=\"_qtyAdd\">\n                            <a class=\"button button--primary card-figcaption-button\">{{lang 'products.add_to_cart'}}</a>\n                        </div>\n                    {{/if}}\n                {{/or}}\n\n                {{#if show_compare}}\n                    <button type=\"button\" class=\"button card-figcaption-button _compare\" disabled><svg class=\"icon\"><use xlink:href=\"#icon-compare\"></use></svg>{{lang 'products.compare'}}</button>\n                {{/if}}\n            </div>\n    </div>\n\n    {{#if show_bulk_order}}\n    <div class=\"card-bulkOrder\">\n        <div class=\"card-bulkOrder-fieldRow card-bulkOrder-qtyInCart\">\n            <span class=\"card-bulkOrder-label\">{{lang 'bulk_order.qty_in_cart'}}: </span>\n            <span class=\"card-bulkOrder-value\">0</span>\n        </div>\n        <div class=\"card-bulkOrder-fieldRow card-bulkOrder-qty\">\n            {{#or customer (unless settings.hide_price_from_guests)}}\n                <div class=\"card-bulkOrder-label\">{{lang 'bulk_order.qty'}}: </div>\n                <div class=\"card-bulkOrder-value\">\n                    <div class=\"form-increment\" data-quantity-change>\n                        <button class=\"button button--icon\" data-action=\"dec\" type=\"button\" disabled>\n                            <i class=\"icon\" aria-hidden=\"true\">\n                                <svg>\n                                    <use xlink:href=\"#icon-minus\"/>\n                                </svg>\n                            </i>\n                        </button>\n                        <input class=\"form-input form-input--incrementTotal\"\n                            name=\"\"\n                            type=\"tel\"\n                            value=\"0\"\n                            data-quantity-min=\"0\"\n                            data-quantity-max=\"0\"\n                            min=\"0\"\n                            pattern=\"[0-9]*\"\n                            aria-live=\"polite\">\n                        <button class=\"button button--icon\" data-action=\"inc\" type=\"button\" disabled>\n                            <i class=\"icon\" aria-hidden=\"true\">\n                                <svg>\n                                    <use xlink:href=\"#icon-add\"/>\n                                </svg>\n                            </i>\n                        </button>\n                    </div>\n                </div>\n            {{/or}}\n        </div>\n        <div class=\"card-bulkOrder-fieldRow card-bulkOrder-price\">\n            <div class=\"card-bulkOrder-label\">{{lang 'bulk_order.price'}}: </div>\n            <div class=\"card-bulkOrder-value\">\n                <div class=\"card-text card-text--price\" data-test-info-type=\"price\">\n                </div>\n            </div>\n        </div>\n        <div class=\"card-bulkOrder-fieldRow card-bulkOrder-subtotal\">\n                <div class=\"card-bulkOrder-label\">{{lang 'bulk_order.subtotal'}}: </div>\n                <div class=\"card-bulkOrder-value\">\n                    <div class=\"card-text card-text--price\">\n                        <span class=\"price price--main\"></span>\n                    </div>\n                </div>\n        </div>\n        <div class=\"card-bulkOrder-action\">\n            {{#if show_cart_action}}\n                <a class=\"button button--small button--primary card-figcaption-button\">{{lang 'products.add_to_cart'}}</a>\n            {{/if}}\n        </div>\n    </div>\n    {{/if}}\n</article>\n"}
