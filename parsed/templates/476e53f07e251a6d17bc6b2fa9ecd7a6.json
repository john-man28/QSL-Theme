{"components/category/shop-by-price":"{{#if shop_by_price}}\n    <div class=\"sidebarBlock sidebarBlock--shopByPrice sidebarBlock--mobileCollapsible\">\n        <h3 class=\"sidebarBlock-heading is-open\" data-collapsible=\"#shopByPrice-navList\" tabindex=\"0\" aria-expanded=\"true\">\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use xlink:href=\"#icon-add\" />\n            </svg>\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use xlink:href=\"#icon-minus\" />\n            </svg>\n            {{lang 'category.shop_by_price'}}\n        </h3>\n        {{#if theme_settings.show_shop_by_price '==' 'slider'}}\n            <div id=\"shopByPrice-navList\" class=\"beautify__navList is-open\" data-beautify-shop-by-price=\"{{JSONstringify shop_by_price}}\" aria-hidden=\"false\">\n                <form class=\"form\" method=\"get\" novalidate>\n                    <fieldset class=\"form-fieldset\">\n                        <div class=\"form-minMaxRow\">\n                            <div class=\"form-field form-field--input\">\n                                <input\n                                    name=\"min_price\"\n                                    placeholder=\"{{lang 'search.faceted.range.min-placeholder'}}\"\n                                    min=\"0\"\n                                    class=\"form-input form-input--small\"\n                                    required\n                                    type=\"number\"\n                                    value=\"\"\n                                /><span class=\"_currency\">{{settings.money.currency_token}}</span>\n                            </div>\n        \n                            <div class=\"form-field form-field--input\">\n                                <input\n                                    name=\"max_price\"\n                                    placeholder=\"{{lang 'search.faceted.range.max-placeholder'}}\"\n                                    min=\"0\"\n                                    class=\"form-input form-input--small\"\n                                    required\n                                    type=\"number\"\n                                    value=\"\"\n                                /><span class=\"_currency\">{{settings.money.currency_token}}</span>\n                            </div>\n                        </div>\n                    </fieldset>\n                    <div class=\"_wrap-silder-shopByPrice\">\n                        <div class=\"beautify__shopByPrice-slider\" data-slider></div>\n                        <div class=\"form-field\">\n                            <button class=\"button button--primary button--small\" type=\"submit\">\n                                {{lang 'search.faceted.range.apply'}}\n                            </button>\n                        </div>\n                    </div>\n                </form>\n                <div class=\"navList-actions-inline\">\n                    <button class=\"navList-action button\" data-cancel style=\"display:none\">{{lang 'common.cancel'}}</button>\n                    <a href=\"{{category_url}}\" class=\"navList-action button\" data-clear style=\"display:none\">\n                        {{lang 'search.faceted.clear'}}\n                        {{#if layout '==' 'left-sidebar'}}\n                            <svg class=\"icon\"><use xlink:href=\"#icon-close\"></use></svg>\n                        {{/if}}\n                    </a>\n                </div>\n            </div>\n        {{else}}\n            <ul id=\"shopByPrice-navList\" class=\"beautify__navList is-open\" aria-hidden=\"true\">\n                {{#each shop_by_price}}\n                <li class=\"navList-item\">\n                    <a class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}}\" data-instantload href=\"{{url}}\" title=\"{{low.formatted}} - {{high.formatted}}\">{{low.formatted}} - {{high.formatted}}</a>\n                </li>\n                {{/each}}\n\n                {{#any shop_by_price selected=true}}\n                    <li class=\"navList-item\">\n                        <a data-instantload href=\"{{category_url}}\" class=\"navList-action\">\n                            {{lang 'category.reset'}}\n                        </a>\n                    </li>\n                {{/any}}\n            </ul>\n        {{/if}}\n    </div>\n{{/if}}\n"}
