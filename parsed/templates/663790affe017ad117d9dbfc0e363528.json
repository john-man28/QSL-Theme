{"components/common/share":"{{#unless (getVar 'isCrawlerAndSpeedTestMode')}}\n{{#or settings.add_this.buttons settings.facebook_like_button.enabled}}\n    {{assignVar 'encodedUrl' (encodeURI url)}}\n    {{assignVar 'encodedTitle' (encodeURI head.title)}}\n    <div class=\"addthis_toolbox\">\n        {{#if settings.add_this.buttons}}\n            <ul class=\"socialLinks socialLinks--alt\"{{#if idPrefix}} id=\"socialLinks-{{idPrefix}}\"{{/if}}>\n                {{#each settings.add_this.buttons}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        {{#if service '===' 'facebook'}}\n                        <a class=\"socialLinks__link icon icon--{{service}}\"\n                        title=\"{{{capitalize service}}}\"\n                        href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\n                        target=\"_blank\"\n                        rel=\"noopener\"\n                        {{{annotation}}}\n                        >\n                            <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                            <svg>\n                                <use xlink:href=\"#icon-facebook\"/>\n                            </svg>\n                        </a>\n                        {{else if service '===' 'email'}}\n                        <a class=\"socialLinks__link icon icon--{{service}}\"\n                        title=\"{{{capitalize service}}}\"\n                        href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n                        target=\"_self\"\n                        rel=\"noopener\"\n                        {{{annotation}}}\n                        >\n                            <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                            <svg>\n                                <use xlink:href=\"#icon-envelope\"/>\n                            </svg>\n                        </a>\n                        {{else if service '===' 'print'}}\n                        <a class=\"socialLinks__link icon icon--{{service}}\"\n                        title=\"{{{capitalize service}}}\"\n                        onclick=\"window.print();return false;\"\n                        {{{annotation}}}\n                        >\n                            <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                            <svg>\n                                <use xlink:href=\"#icon-print\"/>\n                            </svg>\n                        </a>\n                        {{else if service '===' 'twitter'}}\n                        <a class=\"socialLinks__link icon icon--x\"\n                        href=\"https://x.com/intent/tweet/?text={{getVar 'encodedTitle'}}&amp;url={{getVar 'encodedUrl'}}\"\n                        target=\"_blank\"\n                        rel=\"noopener\"\n                        title=\"X\"\n                        {{{annotation}}}\n                        >\n                            <span class=\"aria-description--hidden\">X</span>\n                            <svg>\n                                <use xlink:href=\"#icon-x\"/>\n                            </svg>\n                        </a>\n                        {{else if service '===' 'linkedin'}}\n                        <a class=\"socialLinks__link icon icon--{{service}}\"\n                        title=\"{{{capitalize service}}}\"\n                        href=\"https://www.linkedin.com/shareArticle?mini=true&amp;url={{getVar 'encodedUrl'}}&amp;title={{getVar 'encodedTitle'}}&amp;summary={{getVar 'encodedTitle'}}&amp;source={{getVar 'encodedUrl'}}\"\n                        target=\"_blank\"\n                        rel=\"noopener\"\n                        {{{annotation}}}\n                        >\n                            <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                            <svg>\n                                <use xlink:href=\"#icon-linkedin\"/>\n                            </svg>\n                        </a>\n                        {{else if service '===' 'pinterest'}}\n                        <a class=\"socialLinks__link icon icon--{{service}}\"\n                        title=\"{{{capitalize service}}}\"\n                        href=\"https://pinterest.com/pin/create/button/?url={{getVar 'encodedUrl'}}&amp;description={{getVar 'encodedTitle'}}\"\n                        target=\"_blank\"\n                        rel=\"noopener\"\n                        {{{annotation}}}\n                        >\n                            <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                            <svg>\n                                <use xlink:href=\"#icon-pinterest\"/>\n                            </svg>\n                        </a>\n                        {{/if}}\n                    </li>\n                {{/each}}\n            </ul>\n        {{/if}}\n        {{#if settings.facebook_like_button.enabled}}\n            <iframe\n                class=\"facebook{{#if settings.facebook_like_button.verb '===' 'recommend'}}Recommend{{else}}Like{{/if}}Btn\"\n                style=\"border:none; overflow:hidden; margin: 0; padding: 0; height: 20px; margin-top: .75rem; max-width: 100%\"\n                src=\"https://www.facebook.com/plugins/like.php?href={{settings.facebook_like_button.href}}&amp;layout=button_count{{#if settings.facebook_like_button.verb '===' 'recommend'}}&amp;action=recommend{{/if}}&amp;colorscheme=light&amp;height=20\"\n                scrolling=\"no\"\n                frameborder=\"0\"\n                allowTransparency=\"true\">\n            </iframe>\n        {{/if}}\n    </div>\n{{/or}}\n{{/unless}}\n","components/papathemes/blog-post-modal":"<div class=\"modal-blogPost\" data-blog-id=\"{{entity_id}}\" >\n    <div class=\"modal-body\">\n        <div class=\"modal-content\">\n            {{{region name=\"blog_post_content--global\"}}}\n            {{{region name=\"blog_post_content\"}}}\n            <article class=\"blog {{#if show_tags}}{{#if blog.post.tags}}blog--has-tags{{/if}}{{/if}} {{#unless hide_social_icons}}blog--has-socials{{/unless}}\">\n                <div class=\"blog-post-body\">\n                    <header class=\"blog-header\">\n                        {{#unless single}}\n                            <a class=\"blog-title\" data-instantload href=\"{{blog.post.url}}\">{{blog.post.title}}</a>  \n                        {{/unless}}\n                    </header>\n                    {{#if blog.post.body}}\n                        <div class=\"blog-date\"><span class=\"blog-date-author\">{{lang 'blog.posted_by' name=blog.post.author}}{{/if}}</span><span class=\"blog-date-date\">{{blog.post.date_published}}{{#if blog.post.author}}</span></div>\n                    {{/if}}\n                    <div class=\"blog-post\">\n                        {{#if single}}\n                            {{{region name=\"blog_post_body\"}}}\n                        {{/if}}\n                        {{#if blog.post.body}}\n                            <div class=\"blog-post-summary\">\n                                {{{blog.post.body}}}\n                            </div>\n                            <button class=\"blog-post-more\">Show more</button>\n                        {{else}}\n                            {{#unless hide_summary}}\n                                <div class=\"blog-summary\">\n                                    {{{blog.post.summary}}}\n                                    {{#if blog.post.show_read_more}}\n                                        &hellip;\n                                    {{/if}}\n                                </div>\n                            {{/unless}}\n\n                            <div class=\"blog-dateContainer\">\n                                {{#unless hide_readmore}}\n                                {{#if blog.post.show_read_more}}\n                                    <div class=\"blog-readmore\"><a data-instantload href=\"{{url}}\">{{lang 'blog.read_more'}}</a></div>\n                                {{/if}}\n                                {{/unless}}\n                                <div class=\"blog-date\"><span class=\"blog-date-date\">{{blog.post.date_published}}{{#if blog.post.author}}</span> <span class=\"blog-date-author\">{{lang 'blog.posted_by' name=blog.post.author}}{{/if}}</span></div>\n                            </div>\n                        {{/if}}\n                    </div>\n\n                    {{#if show_tags}}\n                        {{#if blog.post.tags}}\n                            <ul class=\"tags\">\n                                {{#each blog.post.tags}}\n                                <li class=\"tag\">\n                                    <a data-instantload href=\"{{url}}\">#{{name}}</a>\n                                </li>\n                                {{/each}}\n                            </ul>\n                        {{/if}}\n                    {{/if}}\n                </div>\n                <div class=\"blog-post-footer\">\n                    {{#unless hide_social_icons}}\n                    {{> components/common/share url=blog.post.url}}\n                {{/unless}}\n                <a href=\"{{blog.post.url}}\" aria-label=\"{{blog.post.title}}\" class=\"link-to\" type=\"submit\"><svg class=\"icon\"><use xlink:href=\"#icon-link\"/></svg></a>\n                </div>\n            </article>\n        </div>\n        {{{region name=\"blog_post_content_right\"}}}\n    </div>\n</div>\n"}
