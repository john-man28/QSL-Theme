{
  "components/supermarket/sidebar/brands": "<div class=\"sidebarBlock sidebarBlock--navList sidebarBlock--brands sidebarBlock--mobileCollapsible\">\n    <h3 class=\"sidebarBlock-heading is-open\" href=\"#\" data-collapsible=\"#brands-navList\" tabindex=\"0\" aria-expanded=\"true\">\n        <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--off\">\n            <use href=\"#icon-add\" />\n        </svg>\n        <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--on\">\n            <use href=\"#icon-minus\" />\n        </svg>\n        {{lang 'footer.brands'}}\n    </h3>\n    <ul id=\"brands-navList\" class=\"beautify__navList is-open\" aria-hidden=\"false\">\n        {{#if layout '==' 'left-sidebar'}}\n            {{#each shop_by_brand}}\n            <li class=\"navList-item\"><a class=\"navList-action navList-action--checkbox\" rel=\"nofollow\" data-instantload href=\"{{url}}\" title=\"{{name}}\">{{name}}</a></li>\n            {{/each}}\n        {{else}}\n            {{#each shop_by_brand}}\n            <li class=\"navList-item\"><a class=\"navList-action\" rel=\"nofollow\" data-instantload href=\"{{url}}\" title=\"{{name}}\">{{name}}</a></li>\n            {{/each}}\n        {{/if}}\n        <li class=\"navList-item\"><a class=\"navList-action\" data-instantload href=\"{{urls.brands}}\"><svg class=\"icon\"><use href=\"#icon-more\" /></svg><span>{{lang 'search.faceted.more'}}</span></a></li>\n    </ul>\n</div>"
}