{"components/faceted-search/selected-facets":"<div class=\"facetedSearch-refineFilters sidebarBlock\">\n    <div class=\"_header\">\n        <h3 class=\"sidebarBlock-heading\">\n            {{lang 'search.faceted.filter_by'}}\n        </h3>\n        {{#if remove_all_url}}\n            <li>\n                <a class=\"facetedSearch-clearLink\" href=\"{{ remove_all_url }}\" data-faceted-search-facet>\n                    <span>{{lang 'search.faceted.selected.clear-all'}}</span>\n                    <svg class=\"icon\">\n                        <use xlink:href=\"#icon-close\" />\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n    </div>\n\n    {{#if this.items}}\n        <ul class=\"inlineList inlineList--labels\">\n            {{#each this.items}}\n                <li>\n                    <a\n                        href=\"{{ url }}\"\n                        class=\"facetLabel\"\n                        rel=\"nofollow\"\n                        data-faceted-search-facet>\n                        {{#if facet '===' 'rating'}}\n                            {{lang 'search.faceted.selected.rating-label' rating=title}}\n                        {{else}}\n                            {{{sanitize title}}}\n                        {{/if}}\n\n                        <svg class=\"icon\">\n                            <use xlink:href=\"#icon-close\" />\n                        </svg>\n                    </a>\n                </li>\n            {{/each}}\n            {{#if remove_all_url}}\n                <li>\n                    <a class=\"facetedSearch-clearLink\" href=\"{{ remove_all_url }}\" data-faceted-search-facet>\n                        <span>{{lang 'search.faceted.selected.clear-all'}}</span>\n                    </a>\n                </li>\n            {{/if}}\n        </ul>\n    {{else}}\n        <span class=\"_nofilter\"></span>\n    {{/if}}\n</div>\n"}
