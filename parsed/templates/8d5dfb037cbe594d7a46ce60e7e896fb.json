{
  "components/common/cart-preview": "<div class=\"previewCart\" data-cart-quantity=\"{{cart.quantity}}\" data-cart-price=\"{{cart.grand_total.value}}\" data-cart-formatted=\"{{cart.grand_total.formatted}}\">\n    <div class=\"loadingOverlay\"></div>\n    <div class=\"_header\">\n        <a href=\"#\" class=\"_close is-open\" data-toggle=\"cart-preview-dropdown\"><svg class=\"icon\"><use href=\"#icon-close\"></use></svg><span class=\"sr-only\">{{lang 'common.close'}}</span></a>\n        {{> components/papathemes/cart-popup/page-title}}\n        <div data-papathemes-cart-popup-status>\n            {{> components/papathemes/cart-popup/status-messages}}\n            {{#if in_development}}\n                <div class=\"_statusMsg\">\n                    <div class=\"alertBox alertBox--info\">\n                        <div class=\"alertBox-column alertBox-message\">\n                            ðŸ“¦ Free shipping over 59 Euro - Code: IRISH\n                        </div>\n                    </div>\n                </div>\n            {{/if}}\n        </div>\n    </div>\n    <div class=\"_body\">\n        {{#if cart.items.length}}\n            <div data-papathemes-cart-popup-content>\n                {{> components/papathemes/cart-popup/content}}\n            </div>\n            <div class=\"_cartSuggested\" data-cart-suggested-products>\n                <h3 class=\"_subheading\">{{theme_settings.product_cartsuggested_heading}}</h3>\n                <section class=\"productGrid\"\n                    data-list-name=\"Cart Suggested Products\"\n                >\n                    {{#for 1 4}}\n                        <div class=\"productCarousel-slide\">\n                            {{> components/products/card-placeholder settings=../settings show_rating=../settings.show_product_rating theme_settings=../theme_settings customer=../customer event=\"list\" position=(add @index 1)}}\n                        </div>\n                    {{/for}}\n                </section>\n                <div class=\"actionGroup\">\n                    <a href=\"#\" \n                        class=\"button button--small button--outline loadMore\" \n                        style=\"display: none;\">\n                        {{lang 'papathemes.product_cartsuggested.load_more'}}\n                    </a>\n                    <a href=\"#\" \n                        class=\"button button--small button--outline collapse\" \n                        style=\"display: none;\">\n                        {{lang 'papathemes.product_cartsuggested.collapse'}}\n                    </a>\n                </div>\n            </div>\n            <div class=\"_recentlyViewed\" data-recently-viewed-products-section>\n                <h3 class=\"_subheading\">{{theme_settings.product_recentviewed_heading}}</h3>\n                <section class=\"productCarousel blaze-slider\"\n                    data-list-name=\"Recently Viewed Products\"\n                    data-blaze-slider='{\n                        \"all\": {\n                            \"enableAutoplay\": false,\n                            \"autoplayInterval\": 7000,\n                            \"transitionDuration\": 500,\n                            \"slidesToShow\": 2,\n                            \"slidesToScroll\": 2,\n                            \"slideGap\": \"{{#if theme_settings.card_show_border}}12px{{else}}0px{{/if}}\",\n                            \"loop\": true\n                        },\n                        \"(max-width: 800px)\": {\n                            \"slideGap\": \"{{#if theme_settings.card_show_border}}6px{{else}}0px{{/if}}\"\n                        }\n                    }'\n                >\n                    <div class=\"blaze-container\">\n                        <div class=\"blaze-track-container\">\n                            <div class=\"blaze-track\">\n                                {{#for 1 2}}\n                                    <div class=\"productCarousel-slide\">\n                                        {{> components/products/card-placeholder settings=../settings show_rating=../settings.show_product_rating theme_settings=../theme_settings customer=../customer event=\"list\" position=(add @index 1)}}\n                                    </div>\n                                {{/for}}\n                            </div>\n                        </div>\n                        <div class=\"blaze-pagination\"></div>\n                    </div>\n                </section>\n            </div>\n            <div class=\"_wrapper-block\"></div>\n        {{else}}\n            <div class=\"previewCart-emptyBody\">\n                {{lang 'cart.checkout.empty_cart'}}\n            </div>\n        {{/if}}\n    </div>\n    {{#if cart.items.length}}\n        <div class=\"_footer\">\n            <div class=\"previewCartAction\">\n                <div data-papathemes-cart-popup-totals>\n                    {{> components/papathemes/cart-popup/totals}}\n                </div>\n\n                {{#if cart.show_primary_checkout_button}}\n                    <div class=\"previewCartAction-checkout\">\n                        <a href=\"{{urls.checkout.single_address}}\" class=\"button button--primary\" data-primary-checkout-now-action>\n                            {{lang 'cart.preview.checkout_now'}}\n                        </a>\n                    </div>\n                {{/if}}\n\n                {{#if cart.show_multiple_address_shipping}}\n                    <div class=\"previewCartAction-checkoutMultiple\">\n                        <a href=\"{{urls.checkout.multiple_address}}\">\n                            {{lang 'cart.preview.checkout_multiple'}}\n                        </a>\n                    </div>\n                {{/if}}\n            </div>\n            <div data-papathemes-cart-popup-additional-checkout-buttons>\n                {{> components/papathemes/cart-popup/additional-checkout-buttons}}\n            </div>\n        </div>\n    {{/if}}\n</div>\n",
  "components/papathemes/cart-popup/page-title": "<h2 class=\"_heading\" data-cart-page-title>\n    <span>\n        <span class=\"_label\">{{lang 'cart.preview.label'}} </span> <svg class=\"icon is-srOnly\" aria-hidden=\"true\"><use href=\"#icon-shopping-bag\"></use></svg>{{lang 'cart.preview.qty'}}\n        {{#if cart.quantity '>' 0}}\n            <span>({{cart.quantity}})</span>\n        {{/if}}\n    </span>\n    <a href=\"{{urls.cart}}\"><span>{{lang 'cart.preview.edit_cart'}}</span><svg class=\"icon\" aria-hidden=\"true\"><use href=\"#icon-edit-cart2\"></use></svg></a>\n</h2>",
  "components/papathemes/cart-popup/status-messages": "{{#if cart.status_messages.length '>' 0}}\n    <div class=\"_statusMsg\">\n        {{#each cart.status_messages}}\n            <div class=\"alertBox alertBox--info\">\n                <div class=\"alertBox-column alertBox-message\">\n                    {{{message}}}\n                </div>\n            </div>\n        {{/each}}\n    </div>\n{{/if}}",
  "components/papathemes/cart-popup/content": "<ul class=\"previewCartList\" data-cart-quantity=\"{{cart.quantity}}\" data-cart-price=\"{{cart.grand_total.value}}\" data-cart-formatted=\"{{cart.grand_total.formatted}}\">\n    {{#each cart.items}}\n        <li class=\"previewCartItem\" data-cart-itemid=\"{{id}}\">\n            <div class=\"previewCartItem-image\">\n                {{#if type '==' 'GiftCertificate'}}\n                    <img src=\"{{cdn ../../theme_settings.default_image_gift_certificate}}\" alt=\"GiftCertificate\" title=\"GiftCertificate\">\n                {{else}}\n                    {{> components/common/responsive-img\n                        image=image\n                        fallback_size=../../theme_settings.productthumb_size\n                        lazyload=../../theme_settings.lazyload_mode\n                        default_image=../../theme_settings.default_image_product\n                    }}\n                {{/if}}\n            </div>\n            <div class=\"previewCartItem-content\">\n                {{#if ../theme_settings.card_show_brand}}\n                    <span class=\"previewCartItem-brand\">\n                        {{brand.name}}\n                    </span>\n                {{/if}}\n                <h6 class=\"previewCartItem-name\">\n                    <a href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a>\n                </h6>\n\n                <span class=\"previewCartItem-price\">\n                    {{#or ../customer (unless ../settings.hide_price_from_guests)}}\n                        <span class=\"price {{#if price_discounted}}price--discounted{{/if}}\">{{price.formatted}}</span>\n                        {{#if price_discounted}}\n                            <span class=\"price\">{{price_discounted.formatted}}</span>\n                        {{/if}}\n                    {{else}}\n                        {{> components/common/login-for-pricing}}\n                    {{/or}}\n                </span>\n\n                {{!-- inhealth: hide brand\n                <span class=\"previewCartItem-brand\">\n                    {{brand.name}}\n                </span>\n                --}}\n\n                {{#if ../theme_settings.card_show_sku}}\n                    {{#if sku}}\n                        <dl class=\"definitionList definitionList--previewCartItem\">\n                            <dd class=\"definitionList-value\">{{sku}}</dd>\n                        </dl>\n                    {{/if}}\n                {{/if}}\n\n                {{#if release_date}}\n                    <p class=\"previewCartItem-releaseDate\">({{release_date}})</p>\n                {{/if}}\n\n                {{#if configurable_fields}}\n                    <dl class=\"definitionList definitionList--previewCartItem\">\n                        {{#each configurable_fields}}\n                            <dt class=\"definitionList-key\">{{name}}:</dt>\n                            <dd class=\"definitionList-value\">\n                                {{#if is_file}}\n                                    <a href=\"/viewfile.php?prodfield={{../id}}&cartitem={{../../id}}\">{{value}}</a>\n                                {{else}}\n                                    {{> components/common/product-options}}\n                                {{/if}}\n                            </dd>\n                        {{/each}}\n                    </dl>\n                {{/if}}\n\n                <div class=\"_wrapper-changeOption\">\n                    {{#if options}}\n                        <div class=\"definitionList definitionList--previewCartItem\">\n                            {{#each options}}\n                                <div class=\"definitionList--content\">\n                                    <span class=\"definitionList-key\">{{name}}:</span>\n                                    <span class=\"definitionList-value\">\n                                        {{#if is_file}}\n                                            <a href=\"/viewfile.php?attributeId={{../id}}&cartitem={{../../id}}\">{{value}}</a>\n                                        {{else}}\n                                            {{> components/common/product-options}}\n                                        {{/if}}\n                                    </span>\n                                </div>\n                            {{/each}}\n                        </div>\n                    {{/if}}\n\n                    {{#or options configurable_fields}}\n                        <a class=\"_editLink\" href=\"#\" data-item-edit=\"{{id}}\" data-product-id=\"{{product_id}}\" aria-label=\"{{lang 'products.change_product_options' name=name}}\"><i class=\"icon\" aria-hidden=\"true\">\n                            <svg><use href=\"#icon-edit-cart2\" /></svg>\n                        </i></a>\n                    {{/or}}\n                </div>\n\n                {{#if type '==' 'GiftCertificate'}}\n                    <a href=\"{{edit_url}}\">{{lang 'cart.checkout.change'}}</a>\n                {{/if}}\n                {{> components/cart/item-giftwrap this}}\n                {{#if event_date}}\n                    <dl class=\"cart-item-options\">\n                        <dt class=\"cart-item-option-title\">\n                            {{event_date.name}}\n                        </dt>\n\n                        <dd class=\"cart-item-option-description\">\n                            {{event_date.date}}\n                        </dd>\n                    </dl>\n                {{/if}}\n\n                <div class=\"previewCartItem-qtyRemoveRow\">\n                    <div class=\"previewCartItem-quantity\">\n                        <label class=\"form-label cart-item-label\" for=\"cartpopup-qty-{{id}}\">{{lang 'products.quantity'}}</label>\n                        <div class=\"form-increment\">\n                            {{#if can_modify}}\n                                <button class=\"button button--icon\" data-cart-update data-cart-itemid=\"{{id}}\" data-action=\"dec\">\n                                    <span class=\"is-srOnly\">{{lang 'products.quantity_decrease' name=name}}</span>\n                                    <i class=\"icon\" aria-hidden=\"true\"><svg><use href=\"#icon-minus\" /></svg></i>\n                                </button>\n                            {{/if}}\n                            <input class=\"form-input form-input--incrementTotal cart-item-qty-input\"\n                                   id=\"cartpopup-qty-{{id}}\"\n                                   name=\"qty-{{id}}\"\n                                   type=\"tel\"\n                                   value=\"{{quantity}}\"\n                                   data-quantity-min=\"{{min_purchase_quantity}}\"\n                                   data-quantity-max=\"{{max_purchase_quantity}}\"\n                                   data-quantity-min-error=\"{{lang 'products.quantity_min' quantity=min_purchase_quantity}}\"\n                                   data-quantity-max-error=\"{{lang 'products.quantity_max' quantity=max_purchase_quantity}}\"\n                                   min=\"1\"\n                                   pattern=\"[0-9]*\"\n                                   data-cart-itemid=\"{{id}}\"\n                                   data-action=\"manualQtyChange\"\n                                   aria-label=\"{{name}}\"\n                                   aria-live=\"polite\"{{#unless can_modify}} disabled{{/unless}}>\n                            {{#if can_modify}}\n                                <button class=\"button button--icon\" data-cart-update data-cart-itemid=\"{{id}}\" data-action=\"inc\">\n                                    <span class=\"is-srOnly\">{{lang 'products.quantity_increase' name=name}}</span>\n                                    <i class=\"icon\" aria-hidden=\"true\"><svg><use href=\"#icon-add\" /></svg></i>\n                                </button>\n                            {{/if}}\n                        </div>\n                    </div>\n    \n                    {{#if can_modify}}\n                        <span class=\"previewCartItem-remove\">\n                            <a class=\"previewCartItem-remove-icon\" title=\"{{lang 'cart.remove'}}\" data-cart-remove data-cart-itemid=\"{{id}}\" href=\"#\" data-confirm-delete=\"{{lang 'cart.confirm_delete'}}\" aria-autoclose=\"false\">\n                                <svg class=\"icon\" aria-hidden=\"true\"><use href=\"#icon-recycle\" /></svg>\n                            </a>\n                        </span>\n                    {{/if}}\n                    <div class=\"cart-item-totals\">\n                        <div class=\"cart-item-block cart-item-info cart-item-total\">\n                            <span class=\"cart-item-label\">{{lang 'cart.checkout.total'}}</span>\n                            {{#or ../customer (unless ../settings.hide_price_from_guests)}}\n                                <span class=\"cart-item-value {{#if total_discounted}}price--discounted{{/if}}\">{{total.formatted}}</span>\n                                {{#if total_discounted}}\n                                    <span class=\"cart-item-value\">{{total_discounted.formatted}}</span>\n                                {{/if}}\n                            {{else}}\n                                {{> components/common/login-for-pricing}}\n                            {{/or}}\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </li>\n    {{/each}}\n</ul>\n",
  "components/products/card-placeholder": "<article class=\"card card--placeholder {{#if theme_settings.card_show_border}}_border{{/if}} {{#unless theme_settings.card_show_button}}_hideBtn{{/unless}}\">\n    <figure class=\"card-figure\">\n        <div class=\"card-img-container\">\n            <img class=\"card-image\" src=\"data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==\" alt=\"loading\" {{getVar 'productCardImageSizeAttrs'}} />\n        </div>\n        <figcaption class=\"card-figcaption\">\n            <div class=\"card-figcaption-body\">\n                <div class=\"card-buttons card-buttons--alt\">\n                    {{#unless hide_product_quick_view}}\n                        {{#if theme_settings.show_product_quick_view}}\n                            <a class=\"button card-figcaption-button quickview\" tabindex=\"-1\"><svg class=\"icon\"><use href=\"#icon-bs-search\"></use></svg>{{lang 'products.quick_view'}}</a>\n                        {{/if}}\n                    {{/unless}}\n                    {{#if show_compare}}\n                        <button type=\"button\" class=\"button card-figcaption-button _compare2\" disabled><svg class=\"icon\"><use href=\"#icon-compare\"></use></svg>{{lang 'products.compare'}}</button>\n                    {{/if}}\n                </div>\n            </div>\n        </figcaption>\n    </figure>\n    <div class=\"card-body\">\n        <h3 class=\"card-title\"></h3>\n\n        {{#if show_rating}}\n            <p class=\"card-text card-text--rating\" data-test-info-type=\"productRating\">\n                <span class=\"rating--small\"></span>\n            </p>\n        {{/if}}\n\n        {{#if theme_settings.card_show_sku}}\n            <div class=\"card-text card-text--sku\" data-test-info-type=\"sku\"></div>\n        {{/if}}\n\n        {{#if theme_settings.card_show_brand}}\n            <div class=\"card-text card-text--brand\" data-test-info-type=\"brandName\"></div>\n        {{/if}}\n\n        {{#if theme_settings.card_custom_fields '!==' ''}}\n            <div class=\"card-text card-text--info\">\n                <div>\n                    <span class=\"card-info-name card-info-name\"></span>\n                    <span class=\"card-info-value card-info-value\"></span>\n                </div>\n            </div>\n        {{/if}}\n\n        {{#if theme_settings.card_show_swatches}}\n            <div class=\"card-text card-text--colorswatches\"></div>\n        {{/if}}\n\n        <div class=\"card-text card-text--price\" data-test-info-type=\"price\"></div>\n\n        <div class=\"card-text card-text--summary\" data-test-info-type=\"summary\"></div>\n    </div>\n    <div class=\"card-footer{{#if theme_settings.card_show_button}} _show{{/if}}\">\n        <div class=\"card-buttons\">\n                {{#or customer (unless settings.hide_price_from_guests)}}\n                    {{#if show_cart_action}}\n                        <div class=\"_qtyAdd\">\n                            <a class=\"button button--primary card-figcaption-button\">{{lang 'products.add_to_cart'}}</a>\n                        </div>\n                    {{/if}}\n                {{/or}}\n\n                {{#if show_compare}}\n                    <button type=\"button\" class=\"button card-figcaption-button _compare\" disabled><svg class=\"icon\"><use href=\"#icon-compare\"></use></svg>{{lang 'products.compare'}}</button>\n                {{/if}}\n            </div>\n    </div>\n\n    {{#if show_bulk_order}}\n    <div class=\"card-bulkOrder\">\n        <div class=\"card-bulkOrder-fieldRow card-bulkOrder-qtyInCart\">\n            <span class=\"card-bulkOrder-label\">{{lang 'bulk_order.qty_in_cart'}}: </span>\n            <span class=\"card-bulkOrder-value\">0</span>\n        </div>\n        <div class=\"card-bulkOrder-fieldRow card-bulkOrder-qty\">\n            {{#or customer (unless settings.hide_price_from_guests)}}\n                <div class=\"card-bulkOrder-label\">{{lang 'bulk_order.qty'}}: </div>\n                <div class=\"card-bulkOrder-value\">\n                    <div class=\"form-increment\" data-quantity-change>\n                        <button class=\"button button--icon\" data-action=\"dec\" type=\"button\" disabled>\n                            <i class=\"icon\" aria-hidden=\"true\">\n                                <svg>\n                                    <use href=\"#icon-minus\"/>\n                                </svg>\n                            </i>\n                        </button>\n                        <input class=\"form-input form-input--incrementTotal\"\n                            name=\"\"\n                            type=\"tel\"\n                            value=\"0\"\n                            data-quantity-min=\"0\"\n                            data-quantity-max=\"0\"\n                            min=\"0\"\n                            pattern=\"[0-9]*\"\n                            aria-live=\"polite\">\n                        <button class=\"button button--icon\" data-action=\"inc\" type=\"button\" disabled>\n                            <i class=\"icon\" aria-hidden=\"true\">\n                                <svg>\n                                    <use href=\"#icon-add\"/>\n                                </svg>\n                            </i>\n                        </button>\n                    </div>\n                </div>\n            {{/or}}\n        </div>\n        <div class=\"card-bulkOrder-fieldRow card-bulkOrder-price\">\n            <div class=\"card-bulkOrder-label\">{{lang 'bulk_order.price'}}: </div>\n            <div class=\"card-bulkOrder-value\">\n                <div class=\"card-text card-text--price\" data-test-info-type=\"price\">\n                </div>\n            </div>\n        </div>\n        <div class=\"card-bulkOrder-fieldRow card-bulkOrder-subtotal\">\n                <div class=\"card-bulkOrder-label\">{{lang 'bulk_order.subtotal'}}: </div>\n                <div class=\"card-bulkOrder-value\">\n                    <div class=\"card-text card-text--price\">\n                        <span class=\"price price--main\"></span>\n                    </div>\n                </div>\n        </div>\n        <div class=\"card-bulkOrder-action\">\n            {{#if show_cart_action}}\n                <a class=\"button button--small button--primary card-figcaption-button\">{{lang 'products.add_to_cart'}}</a>\n            {{/if}}\n        </div>\n    </div>\n    {{/if}}\n</article>\n",
  "components/papathemes/cart-popup/totals": "<div class=\"_wrapper-coupon-code\">\n    {{#if cart.coupons}}\n        {{#each cart.coupons}}\n            <div class=\"cart-total cart-total--coupons\">\n                <div class=\"cart-total-label\">\n                    {{lang 'cart.coupons.code_label' code=code}}\n                    <div>\n                        <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\n                    </div>\n                </div>\n                <div class=\"cart-total-value\">\n                    <span>{{discount.formatted}}</span>\n                </div>\n            </div>\n        {{/each}}\n    {{else}}\n        <div class=\"cart-total-label _coupon-code-input\">\n            {{{lang 'cart.coupons.add_title'}}}\n            <button class=\"coupon-code-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}<i><svg class='icon'><use href='#icon-chervon-right-thin'></use></svg></i></button>\n        </div>\n        <div class=\"cart-form coupon-code\" style=\"display: none;\">\n            <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"{{urls.cart}}\">\n                <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.coupons.coupon_code'}}</label>\n                <input class=\"form-input\" data-error=\"{{lang 'cart.coupons.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupons.coupon_code'}}\">\n                <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n                <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n            </form>\n        </div>\n    {{/if}}\n</div>\n<ul class=\"previewCartAction-totals\">\n    {{#each cart.taxes}}\n        <li class=\"cart-total cart-total--taxes\">\n            <div class=\"cart-total-label\">\n                {{name}}:\n            </div>\n            <div class=\"cart-total-value\">\n                <span>{{cost.formatted}}</span>\n            </div>\n        </li>\n    {{/each}}\n    <li class=\"cart-total cart-total--grandTotal\">\n        <div class=\"cart-total-label\">\n            {{lang 'cart.checkout.total'}}:\n        </div>\n        <div class=\"cart-total-value cart-total-grandTotal\">\n            {{#or customer (unless settings.hide_price_from_guests)}}\n                <span>{{cart.grand_total.formatted}}</span>\n            {{else}}\n                {{> components/common/login-for-pricing}}\n            {{/or}}\n        </div>\n    </li>\n</ul>",
  "components/papathemes/cart-popup/additional-checkout-buttons": "{{#or customer (unless settings.hide_price_from_guests)}}\n{{#if cart.additional_checkout_buttons}}\n    <div class=\"previewCart-additionalCheckoutButtons\">\n        <div class=\"_heading\">{{lang 'cart.checkout.or'}}</div>\n        {{#each cart.additional_checkout_buttons}}\n            <div class=\"_item\">\n                {{{this}}}\n            </div>\n        {{/each}}\n    </div>\n{{/if}}\n{{/or}}",
  "components/common/responsive-img": "{{!--\n    This file implements the lazysizes srcset pattern (as seen on\n    https://github.com/aFarkas/lazysizes#modern-transparent-srcset-pattern) for maximum performance on modern browsers\n    and reasonable compatibility with legacy browsers.\n    \n    There are a few important arguments to know about when using this component:\n    `lazyload`:\n        - If set to 'lazyload+lqip', will load a low-res image which will be replaced by lazysizes - ensuring that SOME\n        content is shown immediately on page load. Without LQIP, a transparent gif will be used instead, which reduces\n        the number of requests needed to load the page, making it even faster.\n        - If set to 'lazyload', the browser will not load the full-size image at first, instead the lazysizes plugin will\n            load it progressively after page load. This improves the load speed of the page, but it should be disabled for\n            high-priority above-the-fold images.\n        - If set to 'disabled', will not attempt to lazyload the image and instead tell the browser to load it immediately.\n            Use this for high-priority images that are above the fold.\n    \n    `fallback_size`:\n        This is the size to use for this image in legacy browsers that do not support srcset.\n        Can be defined as a pixel bounding-box size (e.g. \"123x123) or inherent width (e.g. \"123w\").\n    \n    `class`:\n        The CSS classes to add to the image, e.g. \"card-image\". If lazyloading is enabled, all images will have the\n        \"lazyload\" class so that lazysizes will target them automatically.\n    \n    `otherAttributes`:\n        Any other HTML attributes you want on the img tag, for example \"height='100' width='100'\"\n    \n    `default_image`:\n        The default image to use if `image` is undefined or falsy. Should be a theme asset, usually defined as a relative\n        path in the theme config. If a default image is not provided, you'll get an image tag with no image if the primary\n        image is undefined.\n\n    `default_alt`:\n        Default alt text if image is not set. Add by PapaThemes.\n\n    `lqip_size`:\n        If you want to specify a particular size for the LQIP image, you can do so with this argument. A default of 80\n        pixels wide will be used otherwise. This argument has no effect if the lazyload mode is not \"lazyload+lqip\".\n    \n    --}}\n    <img {{#if image}}{{!-- Get the default image for legacy browsers that do not support srcset\n    --}}src=\"{{getImageSrcset image 1x=(default fallback_size '160w')}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\" {{!--\n      Allow lazysizes to generate the 'sizes' attribute automatically --}}data-sizes=\"auto\"\n      {{#unless lazyload '==' 'disabled'}}\n        {{#if lazyload '==' 'lazyload+lqip'}}\n        {{!-- If we want a LQIP, include it as the default srcset (will be overwritten by lazysizes) --}}\n        srcset=\"{{getImageSrcset image 1x=(default lqip_size '80w')}}\"\n        {{else}}\n          {{!-- If we don't want a LQIP, use a 1px transparent gif for the stand-in image\n                This requires no additional network request, so it's fast --}}\n        srcset=\"data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==\"\n        {{/if}}\n    {{/unless}}\n    {{!-- If lazyloading is disabled, just use \"srcset\". If it is not disabled, use \"data-srcset\" so lazysizes will use\n          this after page load --}}\n    {{!--\n    Generate the srcset string using the default sizes defined in the helper. This will allow the browser to\n    pick the best-sized image based on its own understanding of how it will render the page.\n    --}}\n    {{#unless lazyload '==' 'disabled'}}data-{{/unless}}srcset=\"{{getImageSrcset image use_default_sizes=true }}\"\n    {{else if default_image}}src=\"{{cdn default_image}}\" data-no-img alt=\"{{default_alt}}\" title=\"{{default_alt}}\"{{/if}}\n    class=\"{{#unless lazyload '==' 'disabled'}}lazyload{{/unless}}{{#if class}} {{class}}{{/if}}\"\n    {{#if lazyload '==' 'lazyload'}}loading=\"lazy\"{{/if}}\n    {{{otherAttributes}}} />\n",
  "components/common/product-options": "{{#if value '==' 'Yes'}}\n{{lang 'common.yes'}}\n{{else if value '==' 'No'}}\n{{lang 'common.no'}}\n{{else}}\n{{{sanitize value}}}\n{{/if}}",
  "components/common/login-for-pricing": "<a class=\"price--login\" href=\"/login.php\" translate>{{lang 'common.login_for_pricing'}}</a>\n",
  "components/cart/item-giftwrap": "{{#if show_gift_wrapping}}\n    <dl class=\"cart-item-options cart-item-options--giftWrapping {{#if gift_wrapping}}_added{{/if}}\">\n        <dt class=\"cart-item-option-title\">\n            {{lang 'cart.gift_wrapping.title'}}:\n        </dt>\n        {{#if gift_wrapping}}\n            <dd class=\"cart-item-option-description\">\n                {{gift_wrapping.name}}: {{gift_wrapping.price.formatted}}\n                <span class=\"cart-item-option-actions\">\n                    (<a href=\"\" data-item-giftwrap=\"{{id}}\">{{lang 'cart.gift_wrapping.change'}}</a> or <a href=\"{{gift_wrapping.remove_url}}\" onclick=\"return confirm('{{lang 'cart.gift_wrapping.remove_confirm'}}')\">{{lang 'cart.gift_wrapping.remove'}}</a>)\n                </span>\n            </dd>\n            {{#if gift_wrapping.message}}\n                <dt class=\"cart-item-option-title\">\n                    {{lang 'cart.gift_wrapping.gift_message'}}:\n                </dt>\n                <dd class=\"cart-item-option-description\">\n                    {{gift_wrapping.message}}\n                </dd>\n            {{/if}}\n        {{else}}\n            <dd class=\"cart-item-option-description\">\n                <a href=\"\" data-item-giftwrap=\"{{id}}\">{{lang 'cart.gift_wrapping.add'}}</a>\n            </dd>\n        {{/if}}\n    </dl>\n{{/if}}\n"
