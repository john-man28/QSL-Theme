{
  "components/supermarket/search-in-category/sidebar": "<nav>\n    {{#if product_results.faceted_search_enabled}}\n        {{> components/faceted-search/index\n            product_results\n            collapsed=(if theme_settings.categorypage_layout '==' 'fullwidth')\n            layout=theme_settings.categorypage_layout\n            currency_token=settings.money.currency_token\n            show_price_slider=(if theme_settings.show_shop_by_price '==' 'slider')\n        }}\n    {{/if}}\n</nav>\n",
  "components/faceted-search/index": "<div id=\"facetedSearch\" class=\"facetedSearch sidebarBlock\"\n    {{#if shop_by_price}}data-beautify-faceted-shop-by-price=\"{{JSONstringify shop_by_price}}\"{{/if}}\n    >\n    {{#if layout '===' 'fullwidth'}}\n        <h3 class=\"sidebarBlock-heading\">\n            {{lang 'search.faceted.selected.title'}}\n        </h3>\n    {{else}}\n        {{> components/faceted-search/selected-facets this.selected}}\n    {{/if}}\n\n    {{#any facets visible=true}}\n        <div\n            id=\"facetedSearch-navList\"\n            class=\"facetedSearch-navList blocker-container\">\n            <div class=\"_accordion _accordion--navList\">\n                {{#each facets}}\n                    {{#if ../collapsed}}\n                        {{#if (merge this start_collapsed=true)}}{{/if}}\n                    {{/if}}\n                    {{#if partial '==' 'range'}}\n                        {{#if (merge this currency_token=../../currency_token show_price_slider=../../show_price_slider)}}{{/if}}\n                    {{/if}}\n                    {{#if visible}}\n                        {{{dynamicComponent 'components/faceted-search/facets'}}}\n                    {{/if}}\n                {{/each}}\n            </div>\n\n            <div class=\"blocker\" style=\"display: none;\"></div>\n        </div>\n    {{/any}}\n\n    {{#if layout '===' 'fullwidth'}}\n        {{> components/faceted-search/selected-facets this.selected}}\n    {{/if}}\n\n    <div class=\"facetedSearch-bottom\">\n        {{#if this.selected.remove_all_url}}\n            <a class=\"button facetedSearch-clearLink is-open\" href=\"{{ this.selected.remove_all_url }}\" data-faceted-search-facet>\n                {{lang 'search.faceted.selected.clear-all'}}\n            </a>\n        {{/if}}\n        <a href=\"#\" class=\"button button--primary is-open\" data-toggle=\"sidebar-top\">{{lang 'search.faceted.apply_filters'}}</a>\n    </div>\n</div>\n",
  "components/faceted-search/selected-facets": "<div class=\"facetedSearch-refineFilters sidebarBlock\">\n    <div class=\"_header\">\n        <h3 class=\"sidebarBlock-heading\">\n            {{lang 'search.faceted.filter_by'}}\n        </h3>\n        {{#if remove_all_url}}\n            <li>\n                <a class=\"facetedSearch-clearLink\" href=\"{{ remove_all_url }}\" data-faceted-search-facet>\n                    <span>{{lang 'search.faceted.selected.clear-all'}}</span>\n                    <svg class=\"icon\">\n                        <use href=\"#icon-close\" />\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n    </div>\n\n    {{#if this.items}}\n        <ul class=\"inlineList inlineList--labels\">\n            {{#each this.items}}\n                <li>\n                    <a\n                        href=\"{{ url }}\"\n                        class=\"facetLabel\"\n                        rel=\"nofollow\"\n                        data-faceted-search-facet>\n                        {{#if facet '===' 'rating'}}\n                            {{lang 'search.faceted.selected.rating-label' rating=title}}\n                        {{else}}\n                            {{{sanitize title}}}\n                        {{/if}}\n\n                        <svg class=\"icon\">\n                            <use href=\"#icon-close\" />\n                        </svg>\n                    </a>\n                </li>\n            {{/each}}\n            {{#if remove_all_url}}\n                <li>\n                    <a class=\"facetedSearch-clearLink\" href=\"{{ remove_all_url }}\" data-faceted-search-facet>\n                        <span>{{lang 'search.faceted.selected.clear-all'}}</span>\n                    </a>\n                </li>\n            {{/if}}\n        </ul>\n    {{else}}\n        <span class=\"_nofilter\"></span>\n    {{/if}}\n</div>\n",
  "components/faceted-search/facets/range": "<div class=\"_accordion-block\">\n    <div\n        class=\"_accordion-navigation toggleLink {{#unless start_collapsed }} is-open {{/unless}}\"\n        role=\"button\"\n        tabindex=\"0\"\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet }}\">\n        <h4 class=\"_accordion-title\">\n            <span class=\"_show-more-cats\">\n                <svg class=\"icon\"><use href=\"#icon-plus\"></use></svg>\n            </span>\n            <span class=\"_show-less-cats\">\n                <svg class=\"icon\"><use href=\"#icon-minus\"></use></svg>\n            </span>\n            {{ title }}\n        </h4>\n        <div class=\"_accordion-navigation-actions\">\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use href=\"#icon-add\" />\n            </svg>\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use href=\"#icon-minus\" />\n            </svg>\n            {{#if selected}}\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>\n                    <span class=\"is-srOnly\">{{lang 'search.faceted.clear'}}</span>\n                    <span class=\"facetedSearch-clearTitlte _clear-titlePrice\">{{lang 'search.faceted.clear'}}</span><svg class=\"icon\"><use href=\"#icon-close\"></use></svg>\n                </a>\n            {{/if}}\n        </div>\n    </div>\n\n    <div id=\"facetedSearch-content--{{dashcase facet }}\" class=\"_accordion-content {{#unless start_collapsed }}is-open{{/unless}}\">\n        <div class=\"navList-actions-inline\">\n            <button class=\"navList-action\" data-cancel style=\"display:none\">{{lang 'common.cancel'}}</button>\n            <a href=\"{{remove_url}}\" class=\"navList-action {{#unless show_price_slider}}u-hidden{{/unless}}\" data-faceted-search-facet data-clear style=\"display:none\">{{lang 'search.faceted.clear'}}<svg class=\"icon\"><use href=\"#icon-close\"></use></svg></a>\n        </div>\n        <form id=\"facet-range-form\" class=\"form\" method=\"get\" data-faceted-search-range novalidate>\n            <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\">\n            {{#if this.sort}}\n                <input type=\"hidden\" name=\"sort\" value={{this.sort}}>\n            {{/if}}\n            <fieldset class=\"form-fieldset\">\n                <div class=\"form-minMaxRow\">\n                    <div class=\"form-field form-field--input\">\n                        <input\n                            name=\"min_price\"\n                            placeholder=\"{{lang 'search.faceted.range.min-placeholder'}}\"\n                            min=\"0\"\n                            class=\"form-input form-input--small\"\n                            required\n                            type=\"number\"\n                            value=\"{{ min_price }}\"\n                        /><span class=\"_currency\">{{currency_token}}</span>\n                    </div>\n    \n                    <div class=\"form-field form-field--input\">\n                        <input\n                            name=\"max_price\"\n                            placeholder=\"{{lang 'search.faceted.range.max-placeholder'}}\"\n                            min=\"0\"\n                            class=\"form-input form-input--small\"\n                            required\n                            type=\"number\"\n                            value=\"{{ max_price }}\"\n                        /><span class=\"_currency\">{{currency_token}}</span>\n                    </div>\n                </div>\n    \n                <div class=\"form-inlineMessage\"></div>\n            </fieldset>\n            <div class=\"facetedSearch-range-head\">\n                {{#if show_price_slider}}\n                    <div class=\"beautify__shopByPrice-slider\" data-slider></div>\n                {{/if}}\n                <div class=\"form-field\">\n                    <button class=\"button button--primary button--small\" type=\"submit\">\n                        {{lang 'search.faceted.range.apply'}}\n                    </button>\n                </div>\n            </div>\n        </form>\n    </div>\n</div>\n",
  "components/faceted-search/facets/multi": "{{#if items}}\n<div class=\"_accordion-block\">\n    <div\n        class=\"_accordion-navigation toggleLink {{#unless start_collapsed }} is-open {{/unless}}\"\n        role=\"button\"\n        tabindex=\"0\"\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet }}\">\n        <h4 class=\"_accordion-title\">\n            <span class=\"_show-more-cats\">\n                <svg class=\"icon\"><use href=\"#icon-plus\"></use></svg>\n            </span>\n            <span class=\"_show-less-cats\">\n                <svg class=\"icon\"><use href=\"#icon-minus\"></use></svg>\n            </span>\n            {{ title }}\n        </h4>\n\n        <div class=\"_accordion-navigation-actions\">\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use href=\"#icon-add\" />\n            </svg>\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use href=\"#icon-minus\" />\n            </svg>\n            {{#if selected}}\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>\n                    <span class=\"is-srOnly\">{{lang 'search.faceted.clear'}}</span>\n                    <span class=\"facetedSearch-clearTitlte\"></span><svg class=\"icon\"><use href=\"#icon-close\"></use></svg>\n                </a>\n            {{/if}}\n        </div>\n    </div>\n    {{#if selected}}\n        <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>\n            <span class=\"is-srOnly\">{{lang 'search.faceted.clear'}}</span>\n            <span class=\"facetedSearch-clearTitlte\">{{lang 'search.faceted.clear'}}</span><svg class=\"icon\"><use href=\"#icon-close\"></use></svg>\n        </a>\n    {{/if}}\n    <div id=\"facetedSearch-content--{{dashcase facet }}\" class=\"_accordion-content {{#unless start_collapsed }} is-open {{/unless}}\">\n        <div class=\"form-field facetedSearch-filterItems\" id=\"facetedSearch-filterItems--{{dashcase facet }}\">\n            <input class=\"form-input\" type=\"text\" placeholder=\"{{lang 'forms.search.search'}} {{ title }}...\">\n        </div>\n        <ul id=\"facetedSearch-navList--{{dashcase facet }}\" data-facet=\"{{facet}}\" data-formfield=\"facetedSearch-filterItems--{{dashcase facet }}\" data-count-item=\"{{items_count}}\" data-load-result=\"off\" class=\"navList\" data-has-more-results=\"{{ has_more_results }}\">\n            {{#each items}}\n                <li class=\"navList-item\">\n                    <a\n                        href=\"{{ url }}\"\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}}\"\n                        rel=\"nofollow\"\n                        data-faceted-search-facet>\n                        {{{sanitize title}}}\n                        {{#if ../show_product_counts}}\n                            <span class=\"_count-item\">{{ count }}</span>\n                        {{/if}}\n\n                        <span class=\"navList-action-close\" aria-hidden=\"true\">\n                            <svg class=\"icon\">\n                                <use href=\"#icon-close\"/>\n                            </svg>\n                        </span>\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n\n        {{#if show_more_toggle}}\n            <a href=\"#facetedSearch-navList--{{dashcase facet}}\" role=\"button\" class=\"toggleLink\">\n                <svg class=\"icon\">\n                    <use href=\"#icon-more\" />\n                </svg>\n                <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.more'}}</span>\n            </a>\n        {{/if}}\n    </div>\n</div>\n{{/if}}\n",
  "components/faceted-search/facets/rating": "{{#if items}}\n<div class=\"_accordion-block\">\n    <div\n        class=\"_accordion-navigation toggleLink {{#unless start_collapsed }} is-open {{/unless}}\"\n        role=\"button\"\n        tabindex=\"0\"\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet }}\">\n        <h4 class=\"_accordion-title\">\n            <span class=\"_show-more-cats\">\n                <svg class=\"icon\"><use href=\"#icon-plus\"></use></svg>\n            </span>\n            <span class=\"_show-less-cats\">\n                <svg class=\"icon\"><use href=\"#icon-minus\"></use></svg>\n            </span>\n            {{ title }}\n        </h4>\n    </div>\n    <div class=\"_accordion-navigation-actions\">\n        <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--off\">\n            <use href=\"#icon-add\" />\n        </svg>\n        <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--on\">\n            <use href=\"#icon-minus\" />\n        </svg>\n        {{#if selected}}\n            <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>\n                <span class=\"is-srOnly\">{{lang 'search.faceted.clear'}}</span>\n                <span class=\"facetedSearch-clearTitlte\">{{lang 'search.faceted.clear'}}</span><svg class=\"icon\"><use href=\"#icon-close\"></use></svg>\n            </a>\n        {{/if}}\n    </div>\n    <div id=\"facetedSearch-content--{{dashcase facet }}\" class=\"_accordion-content facetedSearch-content--{{dashcase facet}} {{#unless start_collapsed }} is-open {{/unless}}\">\n        <ul class=\"navList\">\n            {{#each items}}\n                <li class=\"navList-item\">\n                    <a href=\"{{ url }}\"\n                       class=\"navList-action {{#if selected }} is-selected {{/if}}\"\n                       rel=\"nofollow\"\n                       data-faceted-search-facet>\n                        <span class=\"rating--small\">\n                            {{> components/products/ratings rating=title}}\n                        </span>\n                        {{!-- lang 'search.faceted.rating.and-up' --}}\n                        {{#if ../show_product_counts}}\n                            <span>{{ count }}</span>\n                        {{/if}}\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n</div>\n{{/if}}\n",
  "components/faceted-search/facets/hierarchy": "{{#if items}}\n<div class=\"_accordion-block\">\n    <div\n        class=\"_accordion-navigation toggleLink {{#unless start_collapsed }} is-open {{/unless}}\"\n        role=\"button\"\n        tabindex=\"0\"\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet }}\">\n        <h4 class=\"_accordion-title\">\n            <span class=\"_show-more-cats\">\n                <svg class=\"icon\"><use href=\"#icon-plus\"></use></svg>\n            </span>\n            <span class=\"_show-less-cats\">\n                <svg class=\"icon\"><use href=\"#icon-minus\"></use></svg>\n            </span>\n            {{ title }}\n        </h4>\n\n        <div class=\"_accordion-navigation-actions\">\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use href=\"#icon-add\" />\n            </svg>\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use href=\"#icon-minus\" />\n            </svg>\n        </div>\n    </div>\n\n    {{#if selected}}\n        <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>\n            <span class=\"is-srOnly\">{{lang 'search.faceted.clear'}}</span>\n            <span class=\"facetedSearch-clearTitlte\">{{lang 'search.faceted.clear'}}</span><svg class=\"icon\"><use href=\"#icon-close\"></use></svg>\n        </a>\n    {{/if}}\n\n    <div id=\"facetedSearch-content--{{dashcase facet }}\" class=\"_accordion-content {{#unless start_collapsed }} is-open {{/unless}}\">\n        <div class=\"form-field facetedSearch-filterItems\" id=\"facetedSearch-filterItems--{{dashcase facet }}\">\n            <input class=\"form-input\" type=\"text\" placeholder=\"{{lang 'forms.search.search'}} {{ title }}...\">\n        </div>\n        <ul id=\"facetedSearch-navList--{{dashcase facet }}\" class=\"navList\" data-facet=\"{{facet}}\" data-has-more-results=\"{{ has_more_results }}\">\n            {{#each items}}\n                <li class=\"navList-item\">\n                    <a\n                        href=\"{{ url }}\"\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\n                        rel=\"nofollow\"\n                        data-id=\"{{ id }}\"\n                        data-faceted-search-facet>\n                        {{{sanitize title}}}\n                        {{#if ../show_product_counts}}\n                            {{#if count}}\n                                <span class=\"_count-item\">{{ count }}</span>\n                            {{/if}}\n                        {{/if}}\n                    </a>\n                    {{#if children}}\n                        {{> components/faceted-search/facets/hierarchy-children children}}\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n\n        {{#if show_more_toggle}}\n            <a href=\"#facetedSearch-navList--{{dashcase facet}}\" role=\"button\" class=\"toggleLink\">\n                <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\n            </a>\n        {{/if}}\n    </div>\n</div>\n{{/if}}\n",
  "components/faceted-search/facets/hierarchy-children": "<ul class=\"navList navList--inner\">\n    {{#each this}}\n        <li class=\"navList-item\">\n            <a\n            href=\"{{url}}\"\n            class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\n            rel=\"nofollow\"\n            data-id=\"{{ id }}\"\n            data-faceted-search-facet>\n                {{{sanitize title}}}\n                {{#if ../show_product_counts}}\n                    {{#if count}}\n                        <span>({{count}})</span>\n                    {{/if}}\n                {{/if}}\n            </a>\n            {{#if children}}\n                {{> components/faceted-search/facets/hierarchy-children children}}\n            {{/if}}\n        </li>\n    {{/each}}\n</ul>\n",
  "components/products/ratings": "{{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n        <span class=\"icon icon--ratingFull\">\n            <svg>\n                <use href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{else}}\n        <span class=\"icon icon--ratingEmpty\">\n            <svg>\n                <use href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{/if}}\n{{/for}}\n{{{snippet 'product_rating'}}}\n"
}