{"components/faceted-search/facets/rating":"{{#if items}}\n<div class=\"_accordion-block\">\n    <div\n        class=\"_accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\n        role=\"button\"\n        tabindex=\"0\"\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet }}\">\n        <h4 class=\"_accordion-title\">\n            <span class=\"_show-more-cats\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-plus\"></use></svg>\n            </span>\n            <span class=\"_show-less-cats\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-minus\"></use></svg>\n            </span>\n            {{ title }}\n        </h4>\n    </div>\n    <div class=\"_accordion-navigation-actions\">\n        <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--off\">\n            <use xlink:href=\"#icon-add\" />\n        </svg>\n        <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--on\">\n            <use xlink:href=\"#icon-minus\" />\n        </svg>\n        {{#if selected}}\n            <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>\n                <span class=\"is-srOnly\">{{lang 'search.faceted.clear'}}</span>\n                <span class=\"facetedSearch-clearTitlte\">{{lang 'search.faceted.clear'}}</span><svg class=\"icon\"><use xlink:href=\"#icon-close\"></use></svg>\n            </a>\n        {{/if}}\n    </div>\n    <div id=\"facetedSearch-content--{{dashcase facet }}\" class=\"_accordion-content facetedSearch-content--{{dashcase facet}} {{#unless ../start_collapsed }} is-open {{/unless}}\">\n        <ul class=\"navList\">\n            {{#each items}}\n                <li class=\"navList-item\">\n                    <a href=\"{{ url }}\"\n                       class=\"navList-action {{#if selected }} is-selected {{/if}}\"\n                       rel=\"nofollow\"\n                       data-faceted-search-facet>\n                        <span class=\"rating--small\">\n                            {{> components/products/ratings rating=title}}\n                        </span>\n                        {{!-- lang 'search.faceted.rating.and-up' --}}\n                        {{#if ../show_product_counts}}\n                            <span>{{ count }}</span>\n                        {{/if}}\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n</div>\n{{/if}}\n","components/products/ratings":"{{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n        <span class=\"icon icon--ratingFull\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{else}}\n        <span class=\"icon icon--ratingEmpty\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{/if}}\n{{/for}}\n{{{snippet 'product_rating'}}}\n"}
